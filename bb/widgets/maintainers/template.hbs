
<div class="panel panel-success">
  <div class="panel-heading">
    Maintainers
  </div>
  <div class="panel-body">
  {{#each collection}}
    <div class="media">
      <a class="pull-left" href="/devs/{{slug}}">
        <img class="media-object" src="" style="width:48px; height:48px" alt="{{name}}">
      </a>
      <div class="media-body">
        <h6 class="media-heading">
          <a href="/devs/{{slug}}">
            {{name}}
          </a>
        </h6>
        @ <a href="{{twitter_url}}" target="_blank">twitter</a>
      </div>
    </div>
  {{/each}}
  </div>
</div>
